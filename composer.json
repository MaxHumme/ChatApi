{
    "name": "max-humme/chat-api",
    "description": "My implementation of the bunq software developer assignment.",
    "keywords": ["bunq", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.3.*",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "behat/behat": "^3.2",
        "behat/web-api-extension": "^1.0@dev",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~0.9",
        "phpspec/phpspec": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "MaxHumme\\ChatApi\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "database/"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
